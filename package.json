    "@octokit/request": "6.2.2",
    "@uiw/codemirror-extensions-hyper-link": "4.13.2",
    "@uiw/codemirror-themes": "4.12.4",
    "@vueuse/core": "9.3.1",
    "codemirror": "6.0.1",
    "crypto-js": "4.1.1",
    "idb": "7.1.1",
    "lodash-es": "4.17.21",
    "vite-plugin-pwa": "0.13.1",
    "vite-svg-loader": "3.6.0",
    "vue": "3.2.41",
    "vue-codemirror": "6.1.1",
