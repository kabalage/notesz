{
  "name": "@kabalage/notesz",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all -p vdev devProxy",
    "vdev": "vercel dev --listen 5001 --local-config ./vercel-dev.json",
    "devProxy": "node scripts/devProxy.js",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
